        <MultiLanguageText Lang="en-US">Error in the measuring system</MultiLanguageText>
        <MultiLanguageText Lang="de-DE">FALSE = no error in the measuring system, TRUE = error in the measuring system</MultiLanguageText>
      </Comment>
    </Member>
    <Member Name="control" Datatype="Bool">
      <AttributeList>
        <BooleanAttribute Name="ExternalWritable" SystemDefined="true">false</BooleanAttribute>
      </AttributeList>
      <Comment>
